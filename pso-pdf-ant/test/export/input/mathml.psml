  <media-fragment id="19564-21" mediatype="application/mathml+xml"><math xmlns="http://www.w3.org/1998/Math/MathML"><mi>y</mi><mo>=</mo><msup><mi>x</mi><mn>2</mn></msup><mo>+</mo><mn>1235</mn></math></media-fragment>
